    <children>
        <ImageView pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@assets/chess-board-game-business-concept-light-shadow_12338-591.jpg" />
            </image>
        </ImageView>
        <Button fx:id="settingButton" layoutX="14.0" layoutY="184.0" mnemonicParsing="false" onAction="#settingsPressed" text="Settings" />
        <Button fx:id="playButton" layoutX="14.0" layoutY="137.0" mnemonicParsing="false" onAction="#playPressed" text="Play" />
